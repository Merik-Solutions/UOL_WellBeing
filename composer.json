{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "laravel-notification-channels/fcm": "~2.0",
        "laravel-notification-channels/twilio": "^3.1",
        "laravel/cashier": "^13.5",
        "laravel/framework": "^8.40",
        "laravel/telescope": "^4.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "^6.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "myfatoorah/laravel-package": "^2.0",
        "orangehill/iseed": "^3.0",
        "php-open-source-saver/jwt-auth": "^1.4",
        "prettus/l5-repository": "^2.7",
        "propaganistas/laravel-phone": "^4.3",
        "realrashid/sweet-alert": "^4.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^4.0",
        "stripe/stripe-php": "^9.6",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "doctrine/dbal": "^3.1",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
         "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N"
            
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
